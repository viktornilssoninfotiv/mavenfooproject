				{
					"name": "Create new Booking",
					"event": [
						{
							"listen": "test",
							"script": {
								"id": "ab85e255-2d15-4deb-9417-8776ab9609da",
								"exec": [
									"// Kontroll av statuskod",
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"var jsonData = pm.response.json();",
									"",
									"// Hämtar request bodyn och sparar i variabel",
									"var request_body = JSON.parse(pm.globals.get(\"request_body\"));",
									"",
									"// Den nya bokningens bokningsid sparas i variabel. ",
									"pm.globals.set(\"bookingID\", pm.response.json().bookingid);",
									"",
									"// Kontrollerar att förnamnet i response bodyn stämmer överens med det förnamn vi skickade in i bodyn",
									"pm.test(\"Check firstname of new Booking\", function () {",
									"    pm.expect(jsonData.booking.firstname).to.eql(\"Sally\");",
									"});",
									"",
									"// Kontrollerar att förnamnet i response bodyn stämmer överens med det förnamn vi skickade in i bodyn med hjälp av variabel",
									"pm.test(\"Check firstname of new Booking\", function () {",
									"    pm.expect(jsonData.booking.firstname).to.eql(request_body.firstname);",
									"});"
								],
								"type": "text/javascript"
							}
						},
						{
							"listen": "prerequest",
							"script": {
								"id": "dded9672-2ccf-44a1-8515-24bc50e934ee",
								"exec": [
									"var request_body = {\r",
									"  \"firstname\" : \"Sally\",\r",
									"  \"lastname\" : \"Brown\",\r",
									"  \"totalprice\" : 111,\r",
									"  \"depositpaid\" : true,\r",
									"  \"additionalneeds\" : \"Breakfast\",\r",
									"  \"bookingdates\" : {\r",
									"    \"checkin\" : \"2020-03-06\",\r",
									"    \"checkout\" : \"2020-03-09\"\r",
									"  }\r",
									"};\r",
									"\r",
									"pm.globals.set(\"request_body\", JSON.stringify(request_body));"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"name": "Content-Type",
								"value": "application/json",
								"type": "text"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{{request_body}}"
						},
						"url": {
							"raw": "{{url}}/booking",
							"host": [
								"{{url}}"
							],
							"path": [
								"booking"
							]
						}
					},
					"response": []
				},
